{
  "name": "gfas-react-components",
  "version": "3.0.9",
  "description": "Client side boilerplate",
  "main": "./lib/components/index.js",
  "module": "./lib/components/index.js",
  "scripts": {
    "build": "NODE_ENV=production && rimraf lib && mkdirp lib && babel ./src/ -d . --ignore __tests__ --out-dir lib",
    "dev": "npm run storybook",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "start": "better-npm-run dev",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "prepublish": "npm run build"
  },
  "betterScripts": {
    "dev": {
      "command": "node ./server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "urm:*",
        "PORT": 3000
      }
    },
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "urm:*"
      }
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jjordy/gfas-react-components.git"
  },
  "author": "Jordan Addison <jordanrileyaddison@gmail.com> (http://github.com/jjordy)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jjordy/Universal-React-Min/issues"
  },
  "homepage": "https://github.com/jjordy/Universal-React-Min",
  "peerDependencies": {
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "styled-components": "^1.4.3"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.2",
    "babel-cli": "^6.23.0",
    "babel-core": "latest",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.8",
    "babel-plugin-dynamic-import-node": "^1.0.0",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "better-npm-run": "0.0.14",
    "enzyme": "^2.6.0",
    "eslint": "^3.9.1",
    "eslint-config-standard": "^6.0.0",
    "eslint-config-standard-react": "^4.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "react": "^15.4.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "styled-components": "^1.4.3",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "yargs": "^6.4.0"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  }
}
