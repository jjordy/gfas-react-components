{
  "presets": [
    ["es2015", "modules"],
    "stage-0", "react"],
  "plugins": [
    "syntax-dynamic-import",
    "transform-runtime",
    "transform-async-to-generator",
    "transform-regenerator"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        "dynamic-import-node",
        ["module-resolver", {
          "root": ["./src"]
        }]
      ]
    },
    "production": {
      presets: [
        ["es2015", {"modules": false, "loose": true}],
        "stage-0", "react"
      ]
    }
  }
}
